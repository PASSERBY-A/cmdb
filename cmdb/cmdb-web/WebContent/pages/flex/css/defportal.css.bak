/*
//------------------------------
//  Pod
//------------------------------
*/
Pod
{
	padding-top: 0px;
	padding-left: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	rounded-bottom-corners: false;
	border-thickness: 1px;
	border-style: solid;
	border-color: #000000;

	header-colors: #666666, #666666; 
	highlight-alphas: .9, 0;
	corner-radius: 0px;
	header-height: 22px;
	shadow-direction: right;
	drop-shadow-color: #999999;
	color:           #ffffff;    
	backgroundColor: #333333;
	titleStyleName : podTitle;
}

.podTitle
{
	fontSize: 14;
	font-weight: bold;
}

MenuBar
{
	backgroundColor:#000000;
	textSelectedColor:#000000;
	textRollOverColor:#000000;
	rollOverColor:#000000;
	menuStyleName: "dropDown";
	disabledColor: #AAB3B3;
}
.dropDown {
	backgroundSize: "100%";  
	backgroundAlpha: 1.0;
	borderStyle: solid;
	borderThickness: 1;
	borderColor: #FFFFFF;
	cornerRadius:0;
	color: #000000;
	textRollOverColor: #FFFFFF;
	textSelectedColor:#FFFFFF;
	rollOverColor: #000000;
	selectionColor: #000000;
}
.menuText{
	backgroundAlpha: 0;
	fontSize:12;
	borderStyle:none;
}

