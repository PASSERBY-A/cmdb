.viewTab 
{
    upSkin:                 ClassReference("TabSkin");
    overSkin:               ClassReference("TabSkin");    
    selectedUpSkin:         ClassReference("TabSkin");   
    selectedOverSkin:       ClassReference("TabSkin");    
    color:                  #cecece;
	fontWeight:             normal;
    tabBackgroundColors:    #6e6e6e, #009dff;
    tabBackgroundAlphas:    0.0, 1.0;       
}

.viewTabs
{
    backgroundColor:          #ededed;    
    tabHeight:                30;
    horizontalGap:            0;  
	fontSize:                 12px;
    selectedTabTextStyleName: selectedTabText;
}

.selectedTabText
{
    fontSize:               12px;
    fontWeight:             bold; 
    color:                  #ffffff;
} 

.viewStyle
{
    backgroundColor:    #333333; 
} 

.radioBtn {
	fillColors: #FFFFFF, #FFFFFF;
	color: #FFFFFF;
	textRollOverColor: #FFFFFF;
	textSelectedColor: #FFFFFF;
	disabledColor: #FFFFFF;
}

.drawerButton {
	fontFamily: Verdana;
	fontSize: 12;
	textAlign: left;
	fillAlphas: 1.0, 1.0;
	/*fillColors: #111111, #999999,  #333333, #AAAAAA;*/
	fillColors: #444444, #AAAAAA, #666666, #CCCCCC;
	color: #FFFFFF;
	textRollOverColor: #FFFFFF;
}
.drawerAccordion {
	borderColor: #CCCCCC;
	borderThickness: 2;
	headerStyleName: drawerAccordionHeader;
}
.drawerAccordionHeader {
	fillAlphas: 1.0, 1.0, 0.82, 0.82;
	fillColors: #444444, #AAAAAA, #666666, #CCCCCC;
	/*   fillColors:#2588D1, #015194, #015194, #015194;*/
	color: #FFFFFF;
	cornerRadius: 4;
	paddingLeft: 10;
	fontFamily: Verdana;
	fontSize: 12;
}
.drawerCloseRight {
	up-skin:Embed(source="../assets/close.png");
	over-skin:Embed(source="../assets/close.png");
	down-skin:Embed(source="../assets/close.png");
	disabled-skin:Embed(source="../assets/close.png");
}
.drawerCloseLeft {
	up-skin:Embed(source="../assets/closeleft.png");
	over-skin:Embed(source="../assets/closeleft.png");
	down-skin:Embed(source="../assets/closeleft.png");
	disabled-skin:Embed(source="../assets/closeleft.png");
}

.gradient   
{   
	border-style: solid;   
	border-thickness: 0;   
	border-skin: ClassReference("GradientBorder");   
	fill-colors: #000000, #AAAAAA;   
	drop-shadow-enabled: true;   
}


/*
//------------------------------
//  VAccordion
//------------------------------
*/
VAccordion {
	headerHeight: 80;
	vertical-gap: 5;
	background-alpha:0;
	open-duration:500;
	border-style:none;
	padding-bottom:5;
	header-style-name: gradientHeader;
}

.gradientHeader {
	font-family:Arial;
	cornerRadii: 0, 0, 10, 10;
	borderColors: #FFFFFF, #FFFFFF;
	overBorderColors: #FFFFFF, #FFFFFF;
	selectedBorderColors: #FFFFFF, #FFFFFF;
	borderThickness: 2;
	borderAlpha: 1;
	fillColors: #FFFFFF, #FFFFFF;
	fillAlphas: 1, 1;
	fillColorRatios: 0, 255;
	overFillColors: #FFFFFF, #BAC2CF;
	overFillAlphas: 1, 1;
	overFillColorRatios: 0, 169;
	selectedFillColors: #FFFFFF, #BAC2CF;
	selectedFillAlphas: 1,1;
	selectedFillColorRatios: 0, 169;
}

TabNavigator {
	tabStyleName: "myTabs";
	firstTabStyleName: "myTabs";
	lastTabStyleName: "myTabs";
	selectedTabTextStyleName: "mySelectedTabs";
}
.myTabs {
	textAlign: left;
	color: #0b333c;	
}
.mySelectedTabs {
	color: #000000;	
}

TitleWindow { 
	borderColor: #000000;
	borderAlpha: 0.7;
	modalTransparencyBlur: 0;
	modalTransparency: 0;
	titleStyleName: "mypanelTitle";
	closeButtonSkin:Embed("../assets/icon/TS_8.png");
	closeButtonDisabledSkin:Embed("../assets/icon/TS_8.png");
	closeButtonDownSkin:Embed("../assets/icon/TS_8.png");
	closeButtonOverSkin:Embed("../assets/icon/TS_14.png");
	closeButtonUpSkin:Embed("../assets/icon/TS_8.png");
}
.mypanelTitle {
	color: #ffffff;
	fontSize: 12;
}

/* Style the Window title message. */
.windowStyles {
	color: black;
	letterSpacing: 2;
}

/* Style the Window status message. */
.windowStatus {
	color: red;
	fontWeight: bold;
}

.LabelBlack {
	color: #000000;
}

Alert{
	borderStyle: solid;
	borderColor: #ffffff;
	borderThickness: 1;
	cornerRadius: 5;
	headerHeight: 26;
	backgroundAlpha: 0.51;
	fontSize: 12;
	backgroundColor: #000000;
	titleStyleName: "mypanelTitle";
	messageStyleName: "msyStyleName";
}
.mypanelTitle {
	letterSpacing: 0;
	color: #ffffff;
	fontSize: 14;
	fontWeight: normal;
}
.msyStyleName {	
	color: #000000;	
}

/*
//------------------------------
//  Rules
//------------------------------
*/
HRule
{
	strokeColor:#5a5a5a;
	strokeWidth:1;
}

/*
//------------------------------
//  Slider
//------------------------------
*/
.LVSlider {
	tickOffset: 0;
	tickLength: 10;
	tickColor: #ffffff;
	showTrackHighlight: true;
	invertThumbDirection: true;
	borderColor: #ffffff;
	themeColor: #ff0000;
	fillColors: #ffffff, #ffffff, #ffffff, #eeeeee;
	labelStyleName: "myvSliderLabelStyle";
}
.RVSlider {
	labelOffset: 13;
	tickOffset: 15;
	tickLength: 10;
	tickColor: #ffffff;
	showTrackHighlight: true;
	invertThumbDirection: false;
	borderColor: #ffffff;
	themeColor: #ff0000;
	fillColors: #ffffff, #ffffff, #ffffff, #eeeeee;
	labelStyleName: "myvSliderLabelStyle";
}
.myvSliderLabelStyle {
	color: #ffffff;
}

LegendItem{	
	font-size:12px;	
}

